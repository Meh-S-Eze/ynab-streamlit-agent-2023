# Application Environment
APP_ENV=development

# Logging Configuration
LOG_LEVEL=DEBUG

# Feature Flags
FEATURE_AI_ANALYSIS=true
FEATURE_BUDGET_FORECASTING=true

# Circuit Breaker Configurations
CIRCUIT_BREAKER_YNAB_MAX_FAILURES=3
CIRCUIT_BREAKER_GEMINI_MAX_FAILURES=5

# Caching Configurations
CACHE_BUDGET_QUERIES_MAX_SIZE=32
CACHE_BUDGET_QUERIES_TTL=3600 